//================================
//  Consonants
//================================

.group b
        b          b
     _) b          _b

.group c
        c          k
        c (_       kh
        ch         c         // try [tS;] [tS] [c]

.group d
        d          J         // try [z] [J] [dZ] [dZ;] [J^]

.group đ
        đ          d
     _) đ          _d

.group f
        f          f


.group g
        g          Q         // try [g] [Q]
        gh         Q         // or [g]  [Q]
        g (i       D         // try [D] [z] [j] [J^] [z;] [J] [dZ]
        g (ì       D
        g (í       D
        g (ỉ       D
        g (ĩ       D
        g (ị       D
        gi         D

.group h
        h          h


.group j
        j          Z


.group k
        k          k
        k (_       kh
        kh         x

.group l
        l          l

.group m
        m          m

.group n
        n          n
        ng         N
        ngh        N
        nh         n^

.group ñ
        ñ          n^

.group p
        p          p
        ph         f

.group q
        q          k
        q (_       kh
        qu         kw

.group r
        r          z.        // try [z] [z.] [r] [z;]

.group s
        s          s.        // try [s.] [s] [S]

.group t
        t          t[
        th         t
        tr         cr        // try [cr] [tr] [tS;] [tS] [c] [dZ] [dz;]

.group v
        v          v

.group w
        w          w

.group x
        x          s

.group z
        z          z


